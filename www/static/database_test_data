拥有表中插入测试数据语句：
insert into having_videos values('333', '0014625047290363e4626ff841e41eeba351286715c6749000', '12', '30%', '3');

select having_videos.created_at, videos.id, name, dir_num, price  from having_videos, videos where having_videos.user_id = '00146173793590430c7bc4336cd4a058b70f3c9f8b40202000' and having_videos.video_id = videos.id;

